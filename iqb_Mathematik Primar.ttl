@base <https://w3id.org/iqb/bista22map/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<ab/0e78d160-88d1-4d33-b944-1dc740c5296b> a skos:Concept ;
    skos:broader <ab/> ;
    skos:definition "Zusammenhänge herstellen. Erkennen mathematischer Zusammenhänge und Verknüpfen von Kenntnissen, Fertigkeiten und Fähigkeiten bei der Bearbeitung mathematischer Aufgabenstellungen"@de ;
    skos:inScheme <> ;
    skos:notation "2" ;
    skos:prefLabel "Anforderungsbereich II"@de .

<ab/88f42165-b003-4895-8ffd-873e5c1c9157> a skos:Concept ;
    skos:broader <ab/> ;
    skos:definition "Reproduzieren. Wiedergabe von Grundwissen, Ausführen von Routinetätigkeiten und direkte Anwendung von grundlegenden Begriffen und Verfahren"@de ;
    skos:inScheme <> ;
    skos:notation "1" ;
    skos:prefLabel "Anforderungsbereich I"@de .

<ab/adee89b5-5200-450a-9027-44cf86fae284> a skos:Concept ;
    skos:broader <ab/> ;
    skos:definition "Verallgemeinern und Reflektieren. Übertragen von Erkenntnissen auf unbekannte Fragestellungen sowie Entwickeln und Reflektieren von Strategien, Begründungen und Folgerungen"@de ;
    skos:inScheme <> ;
    skos:notation "3" ;
    skos:prefLabel "Anforderungsbereich III"@de .

<stdInhalt/0c3f8a68-e0a4-42d7-b2eb-9c2daf4f821b> a skos:Concept ;
    skos:broader <stdInhalt/a7011c86-9da0-4565-aa27-de5558336ef0> ;
    skos:inScheme <> ;
    skos:notation "2.2.1" ;
    skos:prefLabel "verstehen und nutzen (u. a. Flächeninhalte durch Auslegen mit Einheitsquadraten, Rauminhalte durch Messen mit Einheitswürfeln) das Grundprinzip des Messens (nicht-standardisierte und standardisierte Einheitsmaße auswählen, wiederholt nutzen und ggf. in Beziehung zu Untereinheiten setzen),"@de .

<stdInhalt/1106c43e-862d-43a3-9b95-3c8a8ab55803> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.1" ;
    skos:prefLabel "verfügen über ein Operationsverständnis zu den vier Grundrechenarten und erkennen und nutzen die Zusammenhänge zwischen den Operationen,"@de .

<stdInhalt/13433fc1-f72e-420a-873e-ea9dc2f06537> a skos:Concept ;
    skos:broader <stdInhalt/86fab034-282c-4c12-82d6-d1534aa0f65d> ;
    skos:inScheme <> ;
    skos:notation "4.3.2" ;
    skos:prefLabel "erkennen und beschreiben Eigenschaften der Achsensymmetrie und setzen diese mit der Achsenspiegelung in Beziehung,"@de .

<stdInhalt/1cf53191-5358-4da4-8ced-e24ff4c0202a> a skos:Concept ;
    skos:broader <stdInhalt/b6801bf3-7cb0-4ae9-899c-7ae626b3c9e5> ;
    skos:inScheme <> ;
    skos:notation "2.1.3" ;
    skos:prefLabel "entwickeln und nutzen Vorstellungen über Repräsentanten für Standardeinheiten und im Alltag bedeutsame Größen (z. B. Höhe der Tür, Dauer der Schulstunde),"@de .

<stdInhalt/20756fb9-2260-4f14-b1cc-86b529e67492> a skos:Concept ;
    skos:broader <stdInhalt/fc14eb7f-6c25-4c47-89af-c7791dadd4d7> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "erkennen, stellen Gleichheit von mathematischen Ausdrücken dar und nutzen diese (z. B. Zahlen durch verschiedene Terme ausdrücken, Terme vergleichen)."@de .

<stdInhalt/234f44b0-146f-4823-8b29-8f0a958d217a> a skos:Concept ;
    skos:broader <stdInhalt/c074648b-e282-4d73-86f9-cd73de37ba6e> ;
    skos:inScheme <> ;
    skos:notation "5.2.2" ;
    skos:prefLabel "schätzen Chancen für das Eintreten von Ereignissen bei alltäglichen Phänomenen oder einfachen Zufallsexperimenten ein und vergleichen (z. B. ist wahrscheinlicher als, hat größere Chancen als) diese datenbasiert."@de .

<stdInhalt/28f60431-5e27-41e2-bfb1-0a93169254ae> a skos:Concept ;
    skos:broader <stdInhalt/34b493c4-bb4e-45c6-b722-432f238f2509> ;
    skos:inScheme <> ;
    skos:notation "4.2.4" ;
    skos:prefLabel "untersuchen und vergleichen ebene Figuren und Körper untersuchen und vergleichen (ebene Figuren auch hinsichtlich des Umfangs und Flächeninhalts, Körper auch hinsichtlich des Rauminhalts),"@de .

<stdInhalt/2a5a603b-ba6b-4911-a46e-82dc030caf26> a skos:Concept ;
    skos:broader <stdInhalt/57b38151-e767-42ae-8ed5-c081e7328917> ;
    skos:inScheme <> ;
    skos:notation "5.1.3" ;
    skos:prefLabel "interpretieren  Darstellungen von Daten und reflektieren diese kritisch, "@de .

<stdInhalt/2b7ac608-a267-428f-bd21-2f634b73a25f> a skos:Concept ;
    skos:broader <stdInhalt/bf8b04f8-9f6b-430b-8a27-50d704d2dfb6> ;
    skos:inScheme <> ;
    skos:notation "4.1.1" ;
    skos:prefLabel "orientieren sich im Raum (z. B. Wege, Pläne, Ansichten),"@de .

<stdInhalt/3091f6cd-97b9-40c2-9459-fd98a8376a7b> a skos:Concept ;
    skos:broader <stdInhalt/2518e0c9-aca0-4b9c-8657-34141ce360ed> ;
    skos:inScheme <> ;
    skos:notation "3.2.1" ;
    skos:prefLabel "erkennen und beschreiben funktionale Beziehungen in Sachsituationen (z. B. Menge - Preis), "@de .

<stdInhalt/318fa61e-0afd-46bf-9ed0-148b16b600f5> a skos:Concept ;
    skos:broader <stdInhalt/34b493c4-bb4e-45c6-b722-432f238f2509> ;
    skos:inScheme <> ;
    skos:notation "4.2.3" ;
    skos:prefLabel "stellen Modelle von Körpern (Vollmodelle, Flächenmodelle, Kantenmodelle) und ebenen Figuren her und untersuchen (z. B. bauen, legen, zerlegen, zusammenfügen, ausschneiden, falten) diese, auch unter Nutzung digitaler Werkzeuge,"@de .

<stdInhalt/3cce5fd4-7dea-4b2e-968f-488eb0bcb902> a skos:Concept ;
    skos:broader <stdInhalt/e33d02eb-b7c2-4b80-97c7-9874dcb7200e> ;
    skos:inScheme <> ;
    skos:notation "2.3.2" ;
    skos:prefLabel "rechnen in Sachsituationen angemessen mit Näherungswerten und prüfen Ergebnisse auf Plausibilität,"@de .

<stdInhalt/3e1b3551-a892-4274-b0a3-07a304ce0528> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.8" ;
    skos:prefLabel "kontrollieren Lösungen durch geeignete Vorgehensweisen (z. B. Überschlagsrechnung, Umkehroperation)."@de .

<stdInhalt/502be895-3ebf-4a6f-9e8b-f3849d283157> a skos:Concept ;
    skos:broader <stdInhalt/a7011c86-9da0-4565-aa27-de5558336ef0> ;
    skos:inScheme <> ;
    skos:notation "2.2.3" ;
    skos:prefLabel "benennen Größenangaben mit verschiedenen Einheiten und stellen diese in unterschiedlichen Schreibweisen (z. B. 2,5 km | 2500 m | 2 km 500 m) dar."@de .

<stdInhalt/564cdd3f-f37b-4d61-800c-c152a0a2dd41> a skos:Concept ;
    skos:broader <stdInhalt/92c68247-5038-418b-a25a-89242cd1c549> ;
    skos:inScheme <> ;
    skos:notation "1.3.2" ;
    skos:prefLabel "runden und überschlagen sachadäquat."@de .

<stdInhalt/587b8b80-4c19-4984-ba36-e55fbffa8b89> a skos:Concept ;
    skos:broader <stdInhalt/5f7a983a-abd5-448c-80c3-82d628e70d9e> ;
    skos:inScheme <> ;
    skos:notation "1.1.3" ;
    skos:prefLabel "orientieren sich im Zahlenraum bis 1 000 000 (z. B. Zahlen der Größe nach ordnen, Nachbarzahlen bestimmen)."@de .

<stdInhalt/5896f04a-6bdc-4eb0-a169-090cdcb6ca24> a skos:Concept ;
    skos:broader <stdInhalt/c074648b-e282-4d73-86f9-cd73de37ba6e> ;
    skos:inScheme <> ;
    skos:notation "5.2.1" ;
    skos:prefLabel "kennen und nutzen Grundbegriffe zur Beschreibung von Zufallsereignissen (sicher, möglich, unmöglich),"@de .

<stdInhalt/5c6af03a-d227-49c6-bc87-f3b755c35a90> a skos:Concept ;
    skos:broader <stdInhalt/e33d02eb-b7c2-4b80-97c7-9874dcb7200e> ;
    skos:inScheme <> ;
    skos:notation "2.3.3" ;
    skos:prefLabel "lösen Sachaufgaben mit Größen."@de .

<stdInhalt/62297092-6417-456f-b4f5-801f59e572e5> a skos:Concept ;
    skos:broader <stdInhalt/b6801bf3-7cb0-4ae9-899c-7ae626b3c9e5> ;
    skos:inScheme <> ;
    skos:notation "2.1.2" ;
    skos:prefLabel "kennen Standardeinheiten (zu Geldwerten, Längen, Zeitspannen, Hohlmaßen und zur Masse) und setzen diese im jeweiligen Größenbereich zueinander in Beziehung,"@de .

<stdInhalt/65979ea6-3f3a-496c-8398-1fca4a223d54> a skos:Concept ;
    skos:broader <stdInhalt/bf8b04f8-9f6b-430b-8a27-50d704d2dfb6> ;
    skos:inScheme <> ;
    skos:notation "4.1.2" ;
    skos:prefLabel "erkennen, beschreiben und nutzen (z. B. zwei- und dreidimensionale Darstellungen zueinander in Beziehung setzen, wie Bauplan und Bauwerk, Körper und Netz) räumliche Beziehungen,"@de .

<stdInhalt/67f8a823-1885-412f-aae4-bd8f16325037> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.7" ;
    skos:prefLabel "verstehen schriftliche Verfahren der Addition, Subtraktion und Multiplikation, beschreiben den Algorithmus, führen diesen geläufig aus und wenden ihn bei geeigneten Aufgaben an,"@de .

<stdInhalt/686035cc-c98c-4779-99de-b3b287ce357a> a skos:Concept ;
    skos:broader <stdInhalt/57b38151-e767-42ae-8ed5-c081e7328917> ;
    skos:inScheme <> ;
    skos:notation "5.1.2" ;
    skos:prefLabel "stellen Daten in Tabellen, Schaubildern und Diagrammen dar, auch unter Nutzung digitaler Werkzeuge, und entnehmen Informationen aus Tabellen, Schaubildern und Diagrammen,"@de .

<stdInhalt/700c29df-b4a5-4643-8dd5-8ad961393ecd> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.3" ;
    skos:prefLabel "übertragen die Grundaufgaben des Kopfrechnens auf analoge Aufgaben im Zahlenraum bis zur Million,"@de .

<stdInhalt/719d94df-ea7b-4c63-9b0c-8775d4ace646> a skos:Concept ;
    skos:broader <stdInhalt/86fab034-282c-4c12-82d6-d1534aa0f65d> ;
    skos:inScheme <> ;
    skos:notation "4.3.1" ;
    skos:prefLabel "bilden ebene Figuren geometrisch ab (verkleinern, vergrößern und spiegeln),"@de .

<stdInhalt/7aa7f99e-2943-4451-8118-0b2bed18187d> a skos:Concept ;
    skos:broader <stdInhalt/b6801bf3-7cb0-4ae9-899c-7ae626b3c9e5> ;
    skos:inScheme <> ;
    skos:notation "2.1.1" ;
    skos:prefLabel "vergleichen und ordnen Größen (Geldwerte, Längen, Zeitspannen, Massen, Flächeninhalte und Volumina),"@de .

<stdInhalt/86a511af-1fe9-47f0-820e-a293757b8817> a skos:Concept ;
    skos:broader <stdInhalt/fc14eb7f-6c25-4c47-89af-c7791dadd4d7> ;
    skos:inScheme <> ;
    skos:notation "3.1.1" ;
    skos:prefLabel "verstehen und nutzen Strukturen in arithmetischen und geometrischen Darstellungen (z. B. in Zahldarstellungen, Anschauungsmitteln) verstehen und nutzen"@de .

<stdInhalt/87ecf524-0563-4422-a5e7-d20be66131bd> a skos:Concept ;
    skos:broader <stdInhalt/bf8b04f8-9f6b-430b-8a27-50d704d2dfb6> ;
    skos:inScheme <> ;
    skos:notation "4.1.3" ;
    skos:prefLabel "operieren (z. B. zerlegen, falten, drehen, spiegeln, bauen) mit geometrischen Objekten gedanklich."@de .

<stdInhalt/8b6793c6-c80b-49d2-ae3b-6cd54997c257> a skos:Concept ;
    skos:broader <stdInhalt/92c68247-5038-418b-a25a-89242cd1c549> ;
    skos:inScheme <> ;
    skos:notation "1.3.1" ;
    skos:prefLabel "wenden bei Sachaufgaben Rechenoperationen an und beschreiben die Beziehungen zwischen der Sache und den einzelnen Lösungsschritten,"@de .

<stdInhalt/90ffbb5a-89e5-4b0d-b51d-059d90ba718e> a skos:Concept ;
    skos:broader <stdInhalt/e33d02eb-b7c2-4b80-97c7-9874dcb7200e> ;
    skos:inScheme <> ;
    skos:notation "2.3.1" ;
    skos:prefLabel "schätzen Größen sachadäquat und mit Bezug zu geeigneten Repräsentanten,"@de .

<stdInhalt/a2520201-162b-4704-8611-28a970c3a7ea> a skos:Concept ;
    skos:broader <stdInhalt/2518e0c9-aca0-4b9c-8657-34141ce360ed> ;
    skos:inScheme <> ;
    skos:notation "3.2.3" ;
    skos:prefLabel "lösen Sachaufgaben zu funktionalen Zusammenhängen (z. B. Proportionalität)."@de .

<stdInhalt/a26b1139-370b-4d85-9fd0-ba5db58d56c0> a skos:Concept ;
    skos:broader <stdInhalt/57b38151-e767-42ae-8ed5-c081e7328917> ;
    skos:inScheme <> ;
    skos:notation "5.1.1" ;
    skos:prefLabel "planen  einfache Befragungen und erfassen und strukturieren bei Beobachtungen, Untersuchungen und einfachen Experimenten Daten,"@de .

<stdInhalt/a61af624-02f5-469f-b633-42ab1d8b9519> a skos:Concept ;
    skos:broader <stdInhalt/86fab034-282c-4c12-82d6-d1534aa0f65d> ;
    skos:inScheme <> ;
    skos:notation "4.3.3" ;
    skos:prefLabel "erkennen und beschreiben geometrische Abbildungen in der Umwelt oder in Mustern (z. B. in Bandornamenten)."@de .

<stdInhalt/a85307c5-4deb-4fbd-915c-84585c787d5e> a skos:Concept ;
    skos:broader <stdInhalt/34b493c4-bb4e-45c6-b722-432f238f2509> ;
    skos:inScheme <> ;
    skos:notation "4.2.2" ;
    skos:prefLabel "erkennen Körper und ebene Figuren in der Umwelt wieder,"@de .

<stdInhalt/b27d06dd-921c-4005-9e82-e6d42dc7792c> a skos:Concept ;
    skos:broader <stdInhalt/fc14eb7f-6c25-4c47-89af-c7791dadd4d7> ;
    skos:inScheme <> ;
    skos:notation "3.1.2" ;
    skos:prefLabel "erkennen und beschreiben Strukturen in geometrischen und arithmetischen Mustern (z. B. Zahlenfolgen, Pentominos) und nutzen diese in mathematischen Kontexten (z. B. Verschlüsselungen),"@de .

<stdInhalt/b61bf1eb-d2fa-4364-8fad-dd829dc47cc3> a skos:Concept ;
    skos:broader <stdInhalt/34b493c4-bb4e-45c6-b722-432f238f2509> ;
    skos:inScheme <> ;
    skos:notation "4.2.1" ;
    skos:prefLabel "klassifizieren Körper und ebene Figuren nach Eigenschaften, ordnen Fachbegriffe zu und beschreiben Beziehungen zwischen geometrischen Figuren (z. B. Quadrat und Rechteck),"@de .

<stdInhalt/b723db2d-4d41-4ee0-8b80-a95e0762ae57> a skos:Concept ;
    skos:broader <stdInhalt/57b38151-e767-42ae-8ed5-c081e7328917> ;
    skos:inScheme <> ;
    skos:notation "5.1.4" ;
    skos:prefLabel "lösen einfache kombinatorische Fragestellungen durch systematisches Vorgehen (z. B. systematisches Probieren) oder mit Hilfe von heuristischen Hilfsmitteln (z. B. Skizze, Baumdiagramm, Tabelle)."@de .

<stdInhalt/c0c2c45f-71c8-4e34-a7fd-3874ce1c479c> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.2" ;
    skos:prefLabel "beherrschen die Grundaufgaben des Kopfrechnens (u. a. Zahlzerlegungen, Eins-pluseins, Einmaleins) gedächtnismäßig und leiten deren Umkehrungen sicher ab,"@de .

<stdInhalt/c71b7560-45cf-4663-927a-7af1773a681d> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.4" ;
    skos:prefLabel "verstehen mündliche und halbschriftliche Rechenstrategien zu den vier Grundrechenarten und setzen diese flexibel ein,"@de .

<stdInhalt/d5404ae3-649a-4d16-8298-448396243f7a> a skos:Concept ;
    skos:broader <stdInhalt/a7011c86-9da0-4565-aa27-de5558336ef0> ;
    skos:inScheme <> ;
    skos:notation "2.2.2" ;
    skos:prefLabel "messen Längen, Zeitspannen, Massen und Hohlmaße mit geeigneten Einheiten und unterschiedlichen Messgeräten sachgerecht,"@de .

<stdInhalt/d9724ab6-a5b8-4c84-bf66-0d0c6d9ad879> a skos:Concept ;
    skos:broader <stdInhalt/34b493c4-bb4e-45c6-b722-432f238f2509> ;
    skos:inScheme <> ;
    skos:notation "4.2.5" ;
    skos:prefLabel "fertigen Zeichnungen geometrischer Figuren mit und ohne Hilfsmittel an, auch unter Nutzung digitaler Werkzeuge."@de .

<stdInhalt/da92088d-e4c9-4f05-a4eb-a2693ae10f88> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.5" ;
    skos:prefLabel "beschreiben, vergleichen und bewerten verschiedene Rechenwege; finden, erklären und berichtigen Rechenfehler,"@de .

<stdInhalt/dafe2372-d10f-4918-8a57-4b61cc7483c1> a skos:Concept ;
    skos:broader <stdInhalt/b6801bf3-7cb0-4ae9-899c-7ae626b3c9e5> ;
    skos:inScheme <> ;
    skos:notation "2.1.4" ;
    skos:prefLabel "kennen und verstehen im Alltag gebräuchliche einfache Brüche im Zusammenhang mit Größen (z. B. 1/2 m, Dreiviertelstunde, 1/4 l)."@de .

<stdInhalt/decdeaa5-c5b8-4657-a6c9-4788e2ac28f9> a skos:Concept ;
    skos:broader <stdInhalt/5f7a983a-abd5-448c-80c3-82d628e70d9e> ;
    skos:inScheme <> ;
    skos:notation "1.1.2" ;
    skos:prefLabel "stellen Zahlen bis 1 000 000 auf verschiedene Weise dar (z. B. Anschauungsmittel, Stufenschrift, Stellenwerttabelle, Zifferndarstellung) und setzen diese zueinander in Beziehung,"@de .

<stdInhalt/def1f955-0e72-40af-a34f-17e21c9be42a> a skos:Concept ;
    skos:broader <stdInhalt/2518e0c9-aca0-4b9c-8657-34141ce360ed> ;
    skos:inScheme <> ;
    skos:notation "3.2.2" ;
    skos:prefLabel "erkennen, beschreiben und stellen funktionale Beziehungen in Tabellen dar,"@de .

<stdInhalt/fcd0e382-9bbe-47f5-b1da-f9006d28774e> a skos:Concept ;
    skos:broader <stdInhalt/d86720ce-c2f4-400b-964e-cd30b0fd67e0> ;
    skos:inScheme <> ;
    skos:notation "1.2.6" ;
    skos:prefLabel "erkennen, erklären und nutzen Rechengesetze (z. B. Kommutativgesetz: Tauschaufgaben), "@de .

<stdInhalt/fe29cc6c-82ad-4703-9661-ac061b45598e> a skos:Concept ;
    skos:broader <stdInhalt/5f7a983a-abd5-448c-80c3-82d628e70d9e> ;
    skos:inScheme <> ;
    skos:notation "1.1.1" ;
    skos:prefLabel "erkennen, erklären und nutzen den Aufbau des dezimalen Stellenwertsystems (z. B. Bündelungsprinzip, Stellenwertprinzip),"@de .

<stdProzess/0462ef02-afd2-4df8-9e33-eeb52bbb012b> a skos:Concept ;
    skos:broader <stdProzess/998f2498-4d6b-434b-9001-086fb24e061f> ;
    skos:inScheme <> ;
    skos:notation "3.3" ;
    skos:prefLabel "überdenken Vorgehensweisen und passen diese gegebenenfalls an."@de .

<stdProzess/0fab1798-cdfd-42d2-aa68-538c7a687fe3> a skos:Concept ;
    skos:broader <stdProzess/0cd2b674-0284-48c0-ba3b-21c24262622f> ;
    skos:inScheme <> ;
    skos:notation "4.2" ;
