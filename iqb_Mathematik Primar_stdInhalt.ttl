        <9eecdf04-6251-4c2e-8cfe-8c68ebb11b19>,
        <c7df94c8-2183-4b8c-89c4-69f0f49c283b> ;
    skos:prefLabel "Inhaltsbezogene Kompetenzen"@de ;
    skos:topConceptOf <> .

