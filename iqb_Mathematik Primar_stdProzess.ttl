    skos:inScheme <> ;
    skos:notation "6.3" ;
    skos:prefLabel "verwenden mathematische Objekte (z. B. Zahldarstellungen, Terme, Ecken, Kanten, Tabellen, Diagramme) bei der Bearbeitung mathematischer Aufgaben- und Problemstellungen sicher und flexibel,"@de .

<ae085e94-27b0-4706-8a41-64bd498999d8> a skos:Concept ;
    skos:broader <b1c8b4c7-b55e-4889-9a95-4642f26b1be9> ;
    skos:inScheme <> ;
    skos:notation "1.2" ;
    skos:prefLabel "stellen Vermutungen zu mathematischen Zusammenhängen auf, "@de .

<b36931d3-6e09-4aed-86ec-947a2277d78d> a skos:Concept ;
    skos:broader <d491fec7-c4e3-4c35-9617-e3f65525cd1f> ;
    skos:inScheme <> ;
    skos:notation "3.1" ;
    skos:prefLabel "entwickeln Lösungsideen zu Aufgaben, zu denen bislang keine Lösungsroutinen bekannt sind,"@de .

