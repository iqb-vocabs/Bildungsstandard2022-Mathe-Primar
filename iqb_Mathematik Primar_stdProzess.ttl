    skos:broader <dcf5818a-7459-4c56-a82a-740e0e99ca19> ;
    skos:inScheme <> ;
    skos:notation "6.2" ;
    skos:prefLabel "verwenden mathematische Fachbegriffe und Zeichen sachgerecht,"@de .

<a2171d65-9777-4e5c-b1f5-b8bd63a7c6b8> a skos:Concept ;
    skos:broader <e34b2e61-f090-43b1-8daf-a4095affea52> ;
    skos:inScheme <> ;
    skos:notation "4.3" ;
